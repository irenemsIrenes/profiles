URL-REGEX,^https?:\/\/.*\.pinduoduo\.com\/i\/toutiao,REJECT

# peopleapp.com
URL-REGEX,^https:\/\/app\.peopleapp\.com\/Api\/\d+/HomeApi\/getAdvertImage,REJECT
